extends layout

block content
    div.container
        h1.headertext.text-right Перечень доступных для учетной записи HUB-ов
        div.list-group
            each item, i in data
                if (true)
                    a.list-group-item.list-group-item-action(id=i,href='/projects?id='+i)= 'Именной персональный HUB учетной записи ' + item.attributes.name
